{
	"name": "mind-bloom",
	"version": "1.0.0",
	"main": "build/app.js",
	"scripts": {
		"test:db": "npx ts-node scripts/testDatabaseConnection.ts",
		"test:network": "npx ts-node scripts/networkTest.ts",
		"test:postgres": "npx ts-node scripts/postgresTest.ts",
		"daily-quotes": "./bin/daily-quotes.sh",
		"watch": "nodemon src/app.ts",
		"prisma:studio": "prisma studio",
		"migrate:to:supabase": "npx ts-node scripts/migrateToSupabase.ts",
		"start": "node build/src/app.js",
		"dev": "nodemon --exec ts-node src/app.ts",
		"prebuild": "prisma generate",
		"build": "prisma generate && tsc",
		"postbuild": "cp -r src/views src/public build/src/",
		"postinstall": "prisma generate",
		"collect": "ts-node src/collect.ts",
		"analyze": "ts-node src/analyze.ts",
		"clean": "rm -rf ./build",
		"prisma:generate": "prisma generate",
		"prisma:migrate:dev": "prisma migrate dev",
		"prisma:migrate:deploy": "prisma migrate deploy",
		"prisma:seed": "npx ts-node prisma/seed.ts"
	},
	"author": "Meghan31",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"@prisma/client": "^6.6.0",
		"axios": "^1.8.4",
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.5.0",
		"express": "^4.21.2",
		"express-handlebars": "^8.0.1",
		"jsonwebtoken": "^9.0.2",
		"pg": "^8.13.3"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/node": "^22.14.0",
		"@types/pg": "^8.11.11",
		"nodemon": "^3.1.9",
		"prisma": "^6.6.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.3",
		"vitest": "^3.0.7"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
